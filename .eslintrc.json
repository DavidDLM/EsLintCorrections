{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended", "airbnb", "prettier"]
    ,
    "plugins": [
        "react", "prettier"
    ],
    "rules": {
    
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [2, "single", { "avoidEscape": true }],
        "semi": [
            "error",
            "always"
        ],
        "camelcase": "error",
        "no-unused-vars": "warn",
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }
        ],
        "func-names": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off"
        
    }
}
